<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

<parameters>
	<parameter key="aj_template.twig.helpers_extension.class">AJ\Bundle\TemplateBundle\Twig\HelpersExtension</parameter>
	<parameter key="aj_template.twig.templating_extension.class">AJ\Bundle\TemplateBundle\Twig\TemplatingExtension</parameter>
</parameters>
<services>
	
<!-- Templating extension
 --><service id="aj_template.twig.helpers_extension"
 			 class="%aj_template.twig.helpers_extension.class%"
 			 public="false">
		<tag name="twig.extension" />
		<argument type="service" id="aj_template.active_template" />
		<argument type="service" id="service_container" />
	</service>

<!-- Twig extension
 --><service id="aj_template.twig.templating_extension"
 			 class="%aj_template.twig.templating_extension.class%"
 			 public="false">
		<tag name="twig.extension" />
		<argument type="service" id="service_container" />
		<argument type="service" id="twig.loader" />
		<argument type="service" id="aj_template.active_template" />
	</service>
		
</services>
 

</container>